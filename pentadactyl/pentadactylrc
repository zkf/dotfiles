noremap h <Nop>
noremap H <Nop>
noremap n 3h
noremap k n
noremap N H
noremap K N
noremap e 3j
noremap E <C-d>
noremap u 3k
noremap U <C-u>
noremap j u
noremap i 3l
noremap l i
noremap L I
noremap I L

" Show tab numbers
set guioptions=ns

" goto gmail
map gm :tabopen gmail.com<CR>

set fbwhitelist=google.com,youtube.com,rainymood.com,github.com,vimeo.com
