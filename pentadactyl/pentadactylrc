"2.3.1 (created: 2010/04/07 18:56:53)
" vim: set ft=vim:

" it segfaults
set loadplugins^=!'fix-focus'
loadplugins 

map -builtin h <Nop>
map -builtin H <Nop>
map -builtin n 3h
map -builtin k n
map -builtin N H
map -builtin K N
map -builtin e 3j
map -builtin E <C-d>
map -builtin u 3k
map -builtin U <C-u>
map -builtin j u
map -builtin i 3l
map -builtin l i
map -builtin L I
map -builtin I L
map -builtin d :tabclose<CR>
map -builtin <C-e> gt
map -builtin <C-u> gT

" Show tab numbers
set guioptions=ns

" goto gmail
map gm :tabopen gmail.com<CR>

set fbwhitelist=google.com,youtube.com,rainymood.com,github.com,vimeo.com
"set fixfocus-key=<S-Escape>
